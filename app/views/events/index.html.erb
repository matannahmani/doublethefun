<div class="navbar">
  <span><%= render "shared/navbar"%></span>
</div>
<div class="banner">
  <img src="https://s3.scoopwhoop.com/anj2/5cf0ce6db37f122acda1e5be/f3a7dff8-916e-47b8-b319-256bac404182.jpg">
</div>
<div class="container">
  <% if !@events.nil? %>
    <% @events.each do |event|  %>
      <div class="card center">
        <img src="">
        <h2>Event - <%= event.name %></h2>
        <h3>Team - <%= event.team %></h3>
        <h3>Location - <%= event.location %></h3>
        <%= link_to 'Check Event', event_path(event) %>
      </div>
    <% end %>
  <% end %>
</div>
<div class="footer">
  <p> <%= render 'shared/footer' %> </p>
</div>
